#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/plane-hybrid-deployment/node_modules/.pnpm/@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_react@18.3.1__react@18.3.1__typescript@5.8.3/node_modules/@react-router/serve/node_modules:/root/plane-hybrid-deployment/node_modules/.pnpm/@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_react@18.3.1__react@18.3.1__typescript@5.8.3/node_modules/@react-router/node_modules:/root/plane-hybrid-deployment/node_modules/.pnpm/@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_react@18.3.1__react@18.3.1__typescript@5.8.3/node_modules:/root/plane-hybrid-deployment/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/plane-hybrid-deployment/node_modules/.pnpm/@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_react@18.3.1__react@18.3.1__typescript@5.8.3/node_modules/@react-router/serve/node_modules:/root/plane-hybrid-deployment/node_modules/.pnpm/@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_react@18.3.1__react@18.3.1__typescript@5.8.3/node_modules/@react-router/node_modules:/root/plane-hybrid-deployment/node_modules/.pnpm/@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_react@18.3.1__react@18.3.1__typescript@5.8.3/node_modules:/root/plane-hybrid-deployment/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_react@18.3.1__react@18.3.1__typescript@5.8.3/node_modules/@react-router/serve/bin.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_react@18.3.1__react@18.3.1__typescript@5.8.3/node_modules/@react-router/serve/bin.js" "$@"
fi
