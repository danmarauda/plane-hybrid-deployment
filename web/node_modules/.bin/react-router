#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/plane-hybrid-deployment/node_modules/.pnpm/@react-router+dev@7.9.5_@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_r_e926747293870546f2d7c7a4f0606d6b/node_modules/@react-router/dev/node_modules:/root/plane-hybrid-deployment/node_modules/.pnpm/@react-router+dev@7.9.5_@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_r_e926747293870546f2d7c7a4f0606d6b/node_modules/@react-router/node_modules:/root/plane-hybrid-deployment/node_modules/.pnpm/@react-router+dev@7.9.5_@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_r_e926747293870546f2d7c7a4f0606d6b/node_modules:/root/plane-hybrid-deployment/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/plane-hybrid-deployment/node_modules/.pnpm/@react-router+dev@7.9.5_@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_r_e926747293870546f2d7c7a4f0606d6b/node_modules/@react-router/dev/node_modules:/root/plane-hybrid-deployment/node_modules/.pnpm/@react-router+dev@7.9.5_@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_r_e926747293870546f2d7c7a4f0606d6b/node_modules/@react-router/node_modules:/root/plane-hybrid-deployment/node_modules/.pnpm/@react-router+dev@7.9.5_@react-router+serve@7.9.5_react-router@7.9.5_react-dom@18.3.1_r_e926747293870546f2d7c7a4f0606d6b/node_modules:/root/plane-hybrid-deployment/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@react-router/dev/bin.js" "$@"
else
  exec node  "$basedir/../@react-router/dev/bin.js" "$@"
fi
