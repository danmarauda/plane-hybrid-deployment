{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "",
  "outputDirectory": "build/client",
  "installCommand": "",
  "framework": null,
  "redirects": [
    {
      "source": "/god-mode",
      "destination": "/god-mode/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/god-mode/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/god-mode/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/((?!assets/).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
